{
  "name": "nekomi",
  "version": "1.0.0",
  "description": "",
  "repository": "git@github.com:nekomiuwu/star.git",
  "main": "build/index.js",
  "license": "UNLICENSED",
  "keywords": [
    "typescript",
    "sern",
    "discord.js"
  ],
  "scripts": {
    "gen-env": "gen-env-types .env -o ./src/env.d.ts -e .",
    "prisma:gen": "npx prisma generate && npx prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "build": "tsc && tsc-alias",
    "dev2": "ts-node-dev --exit-child -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon src/index.ts",
    "start": "ts-node -r tsconfig-paths/register src/index.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "dotenv": "^16.0.3",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@prisma/engines": "^4.8.1",
    "@sern/handler": "^2.1.1",
    "chalk": "^4.1.2",
    "complexity-report": "2.0.0-alpha",
    "dayjs": "^1.11.7",
    "discord-card-canvas": "^1.2.3",
    "discord.js": "^14.7.1",
    "fs": "0.0.1-security",
    "node-cron": "^3.0.2",
    "tsconfig-paths": "^4.1.2",
    "util": "^0.12.5"
  }
